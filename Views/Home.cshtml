@using System.Globalization
@using Umbraco.Cms.Web.Common.PublishedModels;
@using System.Threading;
@using Menu4Tech.Helper
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
@{
	Layout = "master.cshtml";
	var currentLanguage = Thread.CurrentThread.CurrentCulture.TwoLetterISOLanguageName;
}

<main class="main" id="primary">
            <section class="hero">
                <div class="container">
                    <div class="row">
                        <div class="column">
                            <h1>
                                @Html.Raw(Umbraco.GetDictionaryValue("In 20 minutes"))
                            </h1>
                            <p>@Umbraco.GetDictionaryValue("Being a restaurant owner is challenging, so we made POS software that’s easy for you")</p>
                            <p>@Umbraco.GetDictionaryValue("How easy? In just 20 minutes, your POS will be up and running.")</p>
                            <div class="buttons">
                                <a href="https://www.zmenu.net/blog/@Html.Raw(currentLanguage)/page/in-20-minutes-you-will-have-a-running-pos-solution" target="_blank" class="button">@Umbraco.GetDictionaryValue("See details")</a>
                                <a href="@Html.Raw(TranslationHelper.OverridePathWithCurrentCulture("/JoinBusiness/Login"))" class="button button--accent">@Umbraco.GetDictionaryValue("Let’s start")</a>
                            </div>
                        </div>
                        <div class="column">
                            <picture>
                                <source srcset="/assets/images/hero-mobile.png" media="(max-width: 560px)">
                                <img src="/assets/images/hero-image.png" alt="restaurant POS interface">
                            </picture>
                            <div class="icons-platforms">
                                <img src="/assets/images/icons/apple.svg" alt="apple">
                                <img src="/assets/images/icons/android.svg" alt="android">
                                <img src="/assets/images/icons/windows.svg" alt="windows">
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            <section class="steps">
                <div class="container">
                    <h2 class="center">@Html.Raw(Umbraco.GetDictionaryValue("Only 4 steps"))</h2>
                    <ul class="steps__list">
                        <li>
                            <div class="row">
                                <span class="counter"></span>
                                <h3 class="step__title">@Umbraco.GetDictionaryValue("Registration")</h3>
                            </div>
                            <p class="step__text">@Umbraco.GetDictionaryValue("Quick signup to create your restaurant account")</p>
                        </li>
                        <li>
                            <div class="row">
                                <span class="counter"></span>
                                <h3 class="step__title">@Umbraco.GetDictionaryValue("Customization")</h3>
                            </div>
                            <p class="step__text">@Umbraco.GetDictionaryValue("Input your menu, waiters, and tables or import automatically from your old POS")</p>
                        </li>
                        <li>
                            <div class="row">
                                <span class="counter"></span>
                                <h3 class="step__title">@Umbraco.GetDictionaryValue("Installation")</h3>
                            </div>
                            <p class="step__text">@Umbraco.GetDictionaryValue("Instruct your waiters to install menu4u app or/and Install it on your Windows PC or tablet as a fixed POS")</p>
                        </li>
                        <li>
                            <div class="row">
                                <span class="counter"></span>
                                <h3 class="step__title">@Umbraco.GetDictionaryValue("Launch")</h3>
                            </div>
                            <p class="step__text">@Umbraco.GetDictionaryValue("Few minor printers and payment methods configs")</p>
                        </li>
                    </ul>
                    <div class="buttons row">
                        <a href="@Html.Raw(TranslationHelper.OverridePathWithCurrentCulture("/JoinBusiness/Login"))" class="button button--accent">@Umbraco.GetDictionaryValue("Let’s start")</a>
                        <a href="https://www.zmenu.net/blog/@Html.Raw(currentLanguage)/page/in-20-minutes-you-will-have-a-running-pos-solution" target="_blank" class="button">@Umbraco.GetDictionaryValue("See details")</a>
                    </div>
                </div>
            </section>
            <section class="working">
                <div class="container">
                    <div class="working__inner row">
                        <div class="column">
                            <h2 class="">
                                @Html.Raw(Umbraco.GetDictionaryValue("Working from the waiters phone"))
                            </h2>
                            <p>
                                @Umbraco.GetDictionaryValue("No need to buy extra hardware or manage endless restaurant tablets just to offer quick services to customers. Each waiter installs the POS on his private mobile phone and he’s ready to go")
                            </p>
                            <a href="https://www.zmenu.net/blog/@Html.Raw(currentLanguage)/page/working-from-the-waiter-s-phone" target="_blank" class="button">@Umbraco.GetDictionaryValue("Read in detail")</a>
                        </div>
                        <div class="column">
                            <img class="" src="/assets/images/phone.png" alt="waiter using restaurant POS on phone">
                        </div>
                    </div>
                </div>
            </section>
            <section class="serve" data-mobile-bg="/assets/images/serve-bg.png" data-desktop-bg="">
                <div class="container">
                    <div class="row">
                        <div class="column">
                            <img src="/assets/images/internet.png" alt="waiter using restaurant POS with no internet or electricity">
                        </div>
                        <div class="column">
                            <h2 class="">@Html.Raw(Umbraco.GetDictionaryValue("Continue to serve even if the internet or electricity goes down"))</h2>
                            <p>@Umbraco.GetDictionaryValue("Our POS was designed to continue working even if the internet or electricity goes down without worry. All the data and orders will be automatically synced once the network returns offline mode")</p>
                            <a href="https://www.zmenu.net/blog/@Html.Raw(currentLanguage)/page/continue-to-serve-even-if-the-internet-or-electricity-goes-down" target="_blank" class="button button--accent">@Umbraco.GetDictionaryValue("Read in detail")</a>
