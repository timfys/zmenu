                                <li>@Umbraco.GetDictionaryValue("Work With Any Local Provider")</li>
                                <li>@Umbraco.GetDictionaryValue("Track in Real Time")</li>
                                <li>@Umbraco.GetDictionaryValue("One System, Zero Hassle")</li>
                            </ul>
                            <a href="@Html.Raw(TranslationHelper.OverridePathWithCurrentCulture("/JoinBusiness/Login"))" class="button">@Umbraco.GetDictionaryValue("Open an account")</a>
                        </div>

                    </div>
                </div>
